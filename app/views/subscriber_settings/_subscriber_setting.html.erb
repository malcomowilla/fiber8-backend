<div id="<%= dom_id subscriber_setting %>">
  <p>
    <strong>Prefix:</strong>
    <%= subscriber_setting.prefix %>
  </p>

  <p>
    <strong>Minimum digits:</strong>
    <%= subscriber_setting.minimum_digits %>
  </p>

  <p>
    <strong>Account:</strong>
    <%= subscriber_setting.account_id %>
  </p>

  <p>
    <strong>Use autogenerated number as ppoe username:</strong>
    <%= subscriber_setting.use_autogenerated_number_as_ppoe_username %>
  </p>

  <p>
    <strong>Notify user account created:</strong>
    <%= subscriber_setting.notify_user_account_created %>
  </p>

  <p>
    <strong>Send reminder sms expiring payments:</strong>
    <%= subscriber_setting.send_reminder_sms_expiring_payments %>
  </p>

</div>
